[flake8]
max-line-length = 88
extend-ignore = E203,W503,E501

# flake8-import-order configuration
import-order-style = isort
application-import-names = app

exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .mypy_cache,
    .pytest_cache,
    alembic/versions,
    alembic/env.py

per-file-ignores =
    __init__.py:F401